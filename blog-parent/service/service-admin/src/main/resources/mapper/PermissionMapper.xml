<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.cloud.blog.admin.mapper.PermissionMapper">

    <select id="findPermissionList" resultType="org.cloud.blog.admin.vo.PermissionVo">
        select id as value, name as label from ms_permission where deleted = 0;
    </select>
</mapper>
